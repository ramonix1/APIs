          type: string
        usuarios_id_atualizado:
          type: integer
        modificado_em:
          type: string
    CompanyPost:
      type: object
      properties:
        nome:
          type: string
          maxLength: 255
          example: Empresa Teste
        razao_social:
          type: string
          maxLength: 255
          example: Razão social Teste
        cnpj:
          type: string
          maxLength: 14
          example: 11234567000198
        moeda:
          type: string
          maxLength: 20
          example: Gold
        simbolo_moeda:
          type: string
          example: G$
        conversao_real:
          type: string
          example: 2.2
        id_empresa_parceiro:
          type: integer
          example: 1
    CompanyPut:
